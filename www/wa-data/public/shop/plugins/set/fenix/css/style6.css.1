#shop-set-plugin-complect-block {
	border: 0px;
	padding:0px;
	margin: 50px 0;
}

#shop-set-plugin-complect-block .set-item-name a, #shop-set-plugin-complect-block .set-product-name a {
	color: #656565;
}

#shop-set-plugin-complect-block .set-total-price {
	color: #f9981d;
}

#shop-set-plugin-complect-block .set-item-plus {
	color: #333;
	padding: 32px 20px;
}

#shop-set-plugin-complect-block .set-product-price b, #shop-set-plugin-complect-block .set-item-price b {
	color: #333;
}

#shop-set-plugin-complect-block .set-item-name a:hover, #shop-set-plugin-complect-block .set-product-name a:hover {
    color: #f9981d;
}

/*
