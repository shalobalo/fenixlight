*/


#shop-set-plugin-complect-block button{
  background: #f9981d none repeat scroll 0 0;
  border: 1px solid rgba(255, 162, 0, 0.5);
  border-radius: 10px;
  color: #fff;
  font-size: 16px;
  height: 40px;
  line-height: 38px;
  margin: 10px 0 0;
  padding:0 5px;
}

#shop-set-plugin-complect-block .set-image {
	height:90px;
}

@media screen and (max-width: 1023px) { 
	#showSetsPlugin {
		display: none; 
	}

}